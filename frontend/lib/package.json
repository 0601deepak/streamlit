{
    "name": "@streamlit/lib",
    "version": "1.21.0",
    "private": true,
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "yarn build:src && yarn build:lib-type",
        "build:lib-type": "tsc -P tsconfig.json && tsc-alias -p tsconfig.json",
        "build:src": "env NODE_ENV=\"development\" npx babel --root-mode=upward --out-dir dist --source-maps --extensions \".js,.mjs,.jsx,.ts,.tsx\" src"
    },
    "dependencies": {
        "typescript": "^4.9.5"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "babel-cli": "^6.26.0",
        "babel-plugin-emotion": "^11.0.0",
        "tsc-alias": "^1.8.5",
        "@emotion/babel-plugin": "^11.7.2"
    }
}
