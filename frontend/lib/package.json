{
    "name": "@streamlit/lib",
    "version": "1.21.0",
    "private": true,
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "yarn build:src && yarn build:lib-type && yarn build:proto && yarn build:vendor",
        "build:lib-type": "yarn build:tsc && yarn build:tsc-alias",
        "build:tsc": "tsc -P tsconfig.json",
        "build:tsc-alias": "tsc-alias -p tsconfig.json",
        "build:src": "env NODE_ENV=\"development\" ./node_modules/.bin/babel --root-mode=upward --out-dir dist --source-maps --extensions \".js,.mjs,.jsx,.ts,.tsx\" src",
        "build:proto": "cp ./src/proto* ./dist",
        "build:vendor": "cp -R ./src/vendor ./dist"
    },
    "dependencies": {
        "apache-arrow": "^11.0.0",
        "react-markdown": "^8.0.7",
        "typescript": "^4.9.5"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.4",
        "@emotion/babel-plugin": "^11.7.2",
        "babel-plugin-emotion": "^11.0.0",
        "tsc-alias": "^1.8.5",
        "date-fns": "^2.29.3",
        "date-fns-tz": "^2.0.0"
    }
}
